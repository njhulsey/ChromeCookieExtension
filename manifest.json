{
  "name" : "Cookie Control Center",
  "version" : "0.8",
  "description" : "View all cookies and Data store on this device.",
  "permissions": [ "cookies", "tabs", "http://*/*", "https://*/*","storage" ],
  "icons": { "16": "cookie.png", "48": "cookie.png", "128": "cookie.png" },
  "browser_action": {
    "default_icon": "cookie.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "manifest_version": 2
}
